@model IEnumerable<LesGamblers.Web.Models.Gamblers.CheckGamblersPredictionsViewModel>

@{
    ViewBag.Title = "ListGamblersResults";
}

<br />

<div class="jumbotron text-center">
    <h2>All Gamblers Results</h2>
</div>

<br />

<div class="row">
    @if (this.User.IsInRole(LesGamblers.Common.GlobalConstants.AdministratorRoleName))
    {
        @Html.Action("AllGamblersResultsPartialNoCache")
    }
    else
    {
        @Html.Action("_AllGamblersResultsPartial")
    }
</div>

